{%- style -%}
  /* ==============================================
     MOREA LANDING PAGE - Ensemble 4 Pi√®ces
     Clean, Modest, Conversion-Focused
     ============================================== */

  .lp-ensemble {
    --lp-pink: #d4a5a5;
    --lp-dark: #2d2d2d;
    --lp-light: #faf9f7;
    --lp-white: #ffffff;
    --lp-gray: #6b6b6b;
    --lp-border: #e8e8e8;
  }

  /* ========== HERO SECTION ========== */
  .lp-hero {
    display: grid;
    grid-template-columns: 1fr;
    gap: 3rem;
    padding: 4rem 0;
    background: var(--lp-light);
  }

  @media screen and (min-width: 990px) {
    .lp-hero {
      grid-template-columns: 1fr 1fr;
      gap: 5rem;
      padding: 6rem 0;
      align-items: center;
    }
  }

  .lp-hero__media {
    position: relative;
    border-radius: 16px;
    overflow: hidden;
    aspect-ratio: 4/5;
  }

  .lp-hero__media img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .lp-hero__badge {
    position: absolute;
    top: 1.5rem;
    left: 1.5rem;
    background: var(--lp-pink);
    color: var(--lp-white);
    padding: 0.6rem 1.2rem;
    border-radius: 30px;
    font-size: 1.2rem;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.05em;
  }

  .lp-hero__content {
    display: flex;
    flex-direction: column;
    gap: 1.5rem;
  }

  .lp-hero__eyebrow {
    display: flex;
    align-items: center;
    gap: 1rem;
    color: var(--lp-pink);
    font-size: 1.3rem;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.15em;
  }

  .lp-hero__eyebrow i {
    font-size: 1.8rem;
  }

  .lp-hero__title {
    font-family: 'Bebas Neue', sans-serif;
    font-size: clamp(3rem, 7vw, 5rem);
    font-weight: 400;
    line-height: 1;
    color: var(--lp-dark);
    margin: 0;
  }

  .lp-hero__tagline {
    display: flex;
    flex-wrap: wrap;
    gap: 1rem;
    margin-top: 0.5rem;
  }

  .lp-hero__tag {
    background: var(--lp-white);
    color: var(--lp-dark);
    padding: 0.5rem 1rem;
    border-radius: 20px;
    font-size: 1.2rem;
    font-weight: 500;
    border: 1px solid var(--lp-border);
  }

  .lp-hero__intro {
    font-size: 1.6rem;
    line-height: 1.7;
    color: var(--lp-gray);
    max-width: 500px;
  }

  .lp-hero__highlight {
    background: linear-gradient(135deg, var(--lp-pink) 0%, #e8c4c4 100%);
    color: var(--lp-white);
    padding: 1.5rem 2rem;
    border-radius: 12px;
    font-size: 1.5rem;
    font-weight: 500;
    display: flex;
    align-items: center;
    gap: 1rem;
  }

  .lp-hero__highlight i {
    font-size: 2rem;
  }

  .lp-hero__cta {
    display: inline-flex;
    align-items: center;
    gap: 1rem;
    background: var(--lp-dark);
    color: var(--lp-white);
    padding: 1.6rem 3rem;
    border-radius: 8px;
    font-size: 1.5rem;
    font-weight: 600;
    text-decoration: none;
    text-transform: uppercase;
    letter-spacing: 0.08em;
    transition: all 0.3s ease;
    width: fit-content;
  }

  .lp-hero__cta:hover {
    background: var(--lp-pink);
    transform: translateY(-3px);
    box-shadow: 0 10px 30px rgba(212, 165, 165, 0.4);
  }

  .lp-hero__cta i {
    transition: transform 0.3s ease;
  }

  .lp-hero__cta:hover i {
    transform: translateX(5px);
  }

  /* ========== BENEFITS SECTION ========== */
  .lp-benefits {
    padding: 5rem 0;
    background: var(--lp-white);
  }

  .lp-benefits__header {
    text-align: center;
    margin-bottom: 4rem;
  }

  .lp-benefits__title {
    font-family: 'Bebas Neue', sans-serif;
    font-size: clamp(2.5rem, 5vw, 4rem);
    color: var(--lp-dark);
    margin: 0 0 0.5rem;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 1rem;
  }

  .lp-benefits__title i {
    color: var(--lp-pink);
  }

  .lp-benefits__grid {
    display: grid;
    grid-template-columns: 1fr;
    gap: 2rem;
  }

  @media screen and (min-width: 750px) {
    .lp-benefits__grid {
      grid-template-columns: repeat(2, 1fr);
    }
  }

  .lp-benefit {
    background: var(--lp-light);
    border-radius: 16px;
    padding: 2.5rem;
    display: flex;
    gap: 1.5rem;
    transition: all 0.3s ease;
    border: 1px solid transparent;
  }

  .lp-benefit:hover {
    border-color: var(--lp-pink);
    transform: translateY(-5px);
    box-shadow: 0 15px 40px rgba(0, 0, 0, 0.08);
  }

  .lp-benefit__icon {
    width: 50px;
    height: 50px;
    background: var(--lp-pink);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
  }

  .lp-benefit__icon i {
    color: var(--lp-white);
    font-size: 2rem;
  }

  .lp-benefit__content h3 {
    font-family: 'Bebas Neue', sans-serif;
    font-size: 2rem;
    color: var(--lp-dark);
    margin: 0 0 0.5rem;
  }

  .lp-benefit__content p {
    font-size: 1.4rem;
    line-height: 1.6;
    color: var(--lp-gray);
    margin: 0;
  }

  /* ========== INCLUDES SECTION ========== */
  .lp-includes {
    padding: 5rem 0;
    background: linear-gradient(135deg, var(--lp-light) 0%, #f5e6e6 100%);
  }

  .lp-includes__header {
    text-align: center;
    margin-bottom: 4rem;
  }

  .lp-includes__title {
    font-family: 'Bebas Neue', sans-serif;
    font-size: clamp(2.5rem, 5vw, 4rem);
    color: var(--lp-dark);
    margin: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 1rem;
  }

  .lp-includes__title i {
    color: var(--lp-pink);
  }

  .lp-includes__grid {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 1.5rem;
  }

  @media screen and (min-width: 750px) {
    .lp-includes__grid {
      grid-template-columns: repeat(4, 1fr);
      gap: 2rem;
    }
  }

  .lp-includes__item {
    background: var(--lp-white);
    border-radius: 16px;
    padding: 2rem 1.5rem;
    text-align: center;
    transition: all 0.3s ease;
  }

  .lp-includes__item:hover {
    transform: translateY(-5px);
    box-shadow: 0 15px 40px rgba(0, 0, 0, 0.1);
  }

  .lp-includes__item-icon {
    font-size: 3.5rem;
    margin-bottom: 1rem;
  }

  .lp-includes__item h4 {
    font-family: 'Bebas Neue', sans-serif;
    font-size: 1.6rem;
    color: var(--lp-dark);
    margin: 0;
  }

  /* ========== COLORS SECTION ========== */
  .lp-colors {
    padding: 5rem 0;
    background: var(--lp-white);
  }

  .lp-colors__header {
    text-align: center;
    margin-bottom: 3rem;
  }

  .lp-colors__title {
    font-family: 'Bebas Neue', sans-serif;
    font-size: clamp(2.5rem, 5vw, 4rem);
    color: var(--lp-dark);
    margin: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 1rem;
  }

  .lp-colors__title i {
    color: var(--lp-pink);
  }

  .lp-colors__grid {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    gap: 1.5rem;
  }

  .lp-color-swatch {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 0.8rem;
    padding: 1rem;
    border-radius: 12px;
    transition: all 0.3s ease;
    cursor: pointer;
  }

  .lp-color-swatch:hover {
    transform: scale(1.1);
  }

  .lp-color-swatch__circle {
    width: 60px;
    height: 60px;
    border-radius: 50%;
    border: 3px solid var(--lp-border);
    transition: all 0.3s ease;
  }

  .lp-color-swatch:hover .lp-color-swatch__circle {
    border-color: var(--lp-pink);
    box-shadow: 0 5px 20px rgba(0, 0, 0, 0.15);
  }

  .lp-color-swatch__name {
    font-size: 1.2rem;
    font-weight: 500;
    color: var(--lp-dark);
  }

  /* ========== SIZE GUIDE SECTION ========== */
  .lp-sizes {
    padding: 5rem 0;
    background: var(--lp-light);
  }

  .lp-sizes__header {
    text-align: center;
    margin-bottom: 3rem;
  }

  .lp-sizes__title {
    font-family: 'Bebas Neue', sans-serif;
    font-size: clamp(2.5rem, 5vw, 4rem);
    color: var(--lp-dark);
    margin: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 1rem;
  }

  .lp-sizes__title i {
    color: var(--lp-pink);
  }

  .lp-sizes__tables {
    display: grid;
    grid-template-columns: 1fr;
    gap: 3rem;
  }

  @media screen and (min-width: 990px) {
    .lp-sizes__tables {
      grid-template-columns: repeat(2, 1fr);
    }
  }

  .lp-size-table {
    background: var(--lp-white);
    border-radius: 16px;
    overflow: hidden;
    box-shadow: 0 5px 20px rgba(0, 0, 0, 0.05);
  }

  .lp-size-table__header {
    background: var(--lp-dark);
    color: var(--lp-white);
    padding: 1.5rem 2rem;
    font-family: 'Bebas Neue', sans-serif;
    font-size: 1.8rem;
    display: flex;
    align-items: center;
    gap: 1rem;
  }

  .lp-size-table__header i {
    color: var(--lp-pink);
  }

  .lp-size-table table {
    width: 100%;
    border-collapse: collapse;
  }

  .lp-size-table th,
  .lp-size-table td {
    padding: 1.2rem 1.5rem;
    text-align: center;
    font-size: 1.3rem;
  }

  .lp-size-table th {
    background: var(--lp-light);
    color: var(--lp-dark);
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    font-size: 1.1rem;
  }

  .lp-size-table td {
    color: var(--lp-gray);
    border-bottom: 1px solid var(--lp-border);
  }

  .lp-size-table tr:last-child td {
    border-bottom: none;
  }

  .lp-size-table td:first-child {
    font-weight: 600;
    color: var(--lp-dark);
  }

  /* ========== TRUST SECTION ========== */
  .lp-trust {
    padding: 5rem 0;
    background: var(--lp-white);
  }

  .lp-trust__header {
    text-align: center;
    margin-bottom: 4rem;
  }

  .lp-trust__title {
    font-family: 'Bebas Neue', sans-serif;
    font-size: clamp(2.5rem, 5vw, 4rem);
    color: var(--lp-dark);
    margin: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 1rem;
  }

  .lp-trust__title i {
    color: var(--lp-pink);
  }

  .lp-trust__grid {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 2rem;
  }

  @media screen and (min-width: 750px) {
    .lp-trust__grid {
      grid-template-columns: repeat(4, 1fr);
    }
  }

  .lp-trust__item {
    text-align: center;
    padding: 2rem 1.5rem;
  }

  .lp-trust__item-icon {
    width: 60px;
    height: 60px;
    background: linear-gradient(135deg, var(--lp-pink) 0%, #e8c4c4 100%);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 auto 1.5rem;
  }

  .lp-trust__item-icon i {
    color: var(--lp-white);
    font-size: 2.2rem;
  }

  .lp-trust__item h4 {
    font-family: 'Bebas Neue', sans-serif;
    font-size: 1.6rem;
    color: var(--lp-dark);
    margin: 0 0 0.5rem;
  }

  .lp-trust__item p {
    font-size: 1.3rem;
    line-height: 1.5;
    color: var(--lp-gray);
    margin: 0;
  }

  /* ========== FINAL CTA SECTION ========== */
  .lp-final-cta {
    padding: 6rem 0;
    background: linear-gradient(135deg, var(--lp-dark) 0%, #1a1a1a 100%);
    text-align: center;
  }

  .lp-final-cta__content {
    max-width: 700px;
    margin: 0 auto;
  }

  .lp-final-cta__badge {
    display: inline-flex;
    align-items: center;
    gap: 0.8rem;
    background: var(--lp-pink);
    color: var(--lp-white);
    padding: 0.8rem 1.5rem;
    border-radius: 30px;
    font-size: 1.3rem;
    font-weight: 600;
    margin-bottom: 2rem;
  }

  .lp-final-cta__title {
    font-family: 'Bebas Neue', sans-serif;
    font-size: clamp(3rem, 7vw, 5rem);
    color: var(--lp-white);
    margin: 0 0 1.5rem;
    line-height: 1.1;
  }

  .lp-final-cta__subtitle {
    font-size: 1.8rem;
    color: rgba(255, 255, 255, 0.8);
    margin: 0 0 3rem;
    line-height: 1.6;
  }

  .lp-final-cta__tagline {
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    gap: 2rem;
    margin-bottom: 3rem;
  }

  .lp-final-cta__tagline span {
    font-size: 1.5rem;
    font-weight: 500;
    color: var(--lp-pink);
    text-transform: uppercase;
    letter-spacing: 0.1em;
  }

  .lp-final-cta__button {
    display: inline-flex;
    align-items: center;
    gap: 1rem;
    background: var(--lp-white);
    color: var(--lp-dark);
    padding: 1.8rem 4rem;
    border-radius: 8px;
    font-size: 1.6rem;
    font-weight: 700;
    text-decoration: none;
    text-transform: uppercase;
    letter-spacing: 0.1em;
    transition: all 0.3s ease;
  }

  .lp-final-cta__button:hover {
    background: var(--lp-pink);
    color: var(--lp-white);
    transform: translateY(-3px);
    box-shadow: 0 15px 40px rgba(212, 165, 165, 0.4);
  }

  .lp-final-cta__button i {
    transition: transform 0.3s ease;
  }

  .lp-final-cta__button:hover i {
    transform: translateX(5px);
  }

  .lp-final-cta__payment {
    margin-top: 2rem;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 0.8rem;
    color: rgba(255, 255, 255, 0.7);
    font-size: 1.3rem;
  }

  .lp-final-cta__payment i {
    color: var(--lp-pink);
  }
{%- endstyle -%}

